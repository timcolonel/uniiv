<div class='row'>
  <div class='col-md-10 graph'>
    <div id="canvas-container" data-url='<%= user_graph_data_path %>?semester=<%= @term.semester.id %>&year=<%= @term.year.to_s %>' data-semester='<%= @term.semester.id %>' data-year='<%= @term.year.to_s %>'>
    </div>
  </div>
  <div class='col-md-2 graphsidebar'>
    <div>
      <% perc = (current_user.total_completed_ratio*100).to_i.to_s %>
      <div class="row">
        <div class='col-md-12'>
          <div class='text-center'><%= perc %>% Degree Completion</div>
          <div class='progress'>
            <div class='progress-bar' role='progress-bar' aria-valuenow='<%= perc %>' aria-valuemin='0' aria-valuemax='100' style='width:<%= perc %>%;' rel='tooltip' data-placement='bottom' data-toggle='tooltip' title='<%= perc.to_s + "% complete" %>'>
              <span class='sr-only'><%= perc %>% Complete</span>
            </div>
          </div>
        </div>
      </div>
      <% cred = (current_user.count_completed_credit).to_i.to_s %>
      <% cred2 = 0 %>
      <% current_scenario.taking_courses.each do |tc| %>
          <% cred2 += tc.course.credit %>
      <% end %>
      <% cred2 = cred2.to_i.to_s %>
      <div class="row">
        <div class='col-md-12'>
          <div class='text-center'><%= cred %> Credits Completed</div>
          <div class='progress'>
            <div class='progress-bar' role='progress-bar' style='width:<%= cred %>%;' rel='tooltip' data-placement='bottom' data-toggle='tooltip' title='<%= cred.to_s + " credits taken" %>'>
              <span class='sr-only'><%= cred %>% Complete</span>
            </div>
            <div class='progress-bar progress-bar-warning' role='progress-bar' style='width:<%= cred2 %>%;' rel='tooltip' data-placement='bottom' data-toggle='tooltip' title='<%= cred2.to_s + " credits in-progress" %>'>
              <span class='sr-only'><%= cred2 %>% Complete</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <hr>
    <div>
      <div><h4><%= current_user.university %></h4></div>
      <div><%= @term.semester.name.capitalize + " " + @term.year.to_s %></div>
    </div>
    <div>
      <div id="programsfromuser">
        <% current_scenario.programs.order("type_id ASC, name ASC").each do |p| %>
            <div>
              <small><%= p %></small>
            </div>
        <% end %></div>
      <hr>
      <div id="graph_sidebar_info" class='nano'>
        <div class='content'></div>
      </div>
    </div>
    <%= render :partial => 'graph/graph_navigation' %>
  </div>
</div>
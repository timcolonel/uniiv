<%= form_tag(user_programs_create_path) do %>
    <div class='form-group'>
      <div class='input-group'>

        <%= text_field_tag 'program_id', nil, :class => 'form-control autocomplete disablesubmit', 'data-url' => program_search_autocomplete_path(:only_not_taking => true),
                           'data-input-data' => 'true', :placeholder => t('select.programs') %>
        <span class="input-group-btn">
                <%= submit_tag('Choose', :class => 'btn btn-primary', :disabled=> true) %>
            </span>
      </div>
    </div>
<% end %>
<div class="content_box login_content">
  <h2>Choose your program</h2>
  <%= form_tag(user_programs_create_path) do %>
    <%= select_tag 'prog', options_for_select(@faculty.programs.map{  |x| [x, x.id]}  )  , { :class => "form_input_long middle form_biginput" } %>
    <div class="right">
      <%= submit_tag("Next") %>
    </div>
<% end %>
</div>

